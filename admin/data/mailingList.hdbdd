namespace workshop.admin.data;

@Schema: 'WORKSHOP'
@Catalog.tableType: #COLUMN
     Entity mailingList {
		key "RecordId": String(10); // Record ID
		"SessionId": String(20) not null; // Session ID
		"UserId": String(20) not null; // User ID
		"DateCreated": UTCTimestamp not null; // Date Created
